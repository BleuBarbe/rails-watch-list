<h1>Create your list</h1>


<div class="container">

      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name, placeholder: 'Add a list name' %>
        <%= f.input :image_url, placeholder: 'Add an image URL' %>
        <%= f.submit "Create", class: "btn btn-primary" %>
      <% end %>
</div>
